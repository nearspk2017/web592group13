      <script>
        // hotfix for kids 'keep away from fire' text. Make uppercase and red
        var _pageType = $('#js-pageType').text();
        _infoTab = $('#infotab [title="Details & Care"]').parents('.section').find('.content p').text(),
                _regex = /(KEEP AWAY FROM FIRE)/i;

        if( _pageType == "product") {
          var _newStr = _infoTab.replace(_regex, '<span class="boohooRed" style="text-transform: uppercase;">$1</span>');
          $('#infotab [title="Details & Care"]').parents('.section').find('.content p').html(_newStr);
        }

        jQuery(function($){
          var iOS = /iPad|iPhone|iPod/.test(navigator.userAgent) && !window.MSStream;
          if(iOS && typeof(Venda) !== "undefined" && typeof(Venda.AddressLookup) !== "undefined"){
            Venda.AddressLookup.lookupSelect = function () {
              $('#js-lookupselect').parents('form').validate().resetForm();
              $('#js-hideOrShowAddress').fadeIn(300); // Show the address form
              $('#js-lookupresults').empty(); // Empty the previous lookup results
              $('#js-addresses-found').fadeOut(300); // Hide the address lookup box
              $('#js-lookup-reset-btn').fadeIn(300); // Show the lookup reset button
              $('#js-manual-reset-btn').hide(); // Hide the manual reset button
              $('#js-lookup-submit-btn').hide(); // Hide the lookup submit button
              $('.js-reset-btn').hide();
              Venda.CountrySelect.HighLightRow();
              $('#statetext').click().focus();
            };
          }
        });

        jQuery(function($){
          $('.AUD #js-lookup-submit-btn, .NZD #js-lookup-submit-btn').remove();
        });

      </script>

      <script>

        jQuery(function(){

          var _dragging = false;

          jQuery('body').on('touchmove', 'area', function () {
            _dragging = true;
          });

          jQuery('body').on('touchend', 'area', function () {
            if(_dragging) {
              _dragging = false;
              return;
            }
            window.location = jQuery(this).attr("href");
          });

          boohoo_promoAreas.done(function() {

            var _carouselWrapper = jQuery('[data-zone="homepageboxes-GBP"]'),
                    _carouselItems = _carouselWrapper.find('.orbit-container li');

            if(_carouselItems.length > 0) {
              carouselListeners(_carouselItems);
            } else {
              _carouselWrapper.on('orbit:ready', function() {
                _carouselItems = _carouselWrapper.find('.orbit-container li');
                carouselListeners(_carouselItems);
              });
            }

          });

          function carouselListeners(items) {

            items.on('touchmove', function (e) {
              _dragging = true;
            });

            items.on('touchend', function (e) {

              if(_dragging) {
                _dragging = false;
                return;
              }

              window.location = jQuery(e.target).attr("href");
            });

          }

        });

      </script>
