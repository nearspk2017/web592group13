var boohoo_orditemJSON = function() {
    // since we're re-writing the function not too concerned about defining it within a function
    function unescapeHtml( safe ) {
        return safe.replace( /&amp;/g, '&' )
            .replace( /&lt;/g, '<' )
            .replace( /&gt;/g, '>' )
            .replace( /&quot;/g, '"' )
            .replace( /&#39;/g, "'" );        
    }

