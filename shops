                jQuery( document ).ready(function( $ ) {

                    $("#normalSignUp .button").click(function(){

                        var SessJar = new CookieJar( {expires: '', path: '/' } ),
                            usEmail = Venda.Platform.getUrlParam(location.href, 'email');

                        SessJar.put( 'usEmail', usEmail );

