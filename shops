    <script>
jQuery( document ).ready(function( $ ) {

    $("#normalSignUp .button").click(function(){

        var emailID2 = Venda.Platform.getUrlParam(location.href, 'email');
        var currCode = $('#tag-currencycode').text();

        var objStore = {
            type: 'mergeListMembers',
            source: 'signupForm',
            EMAIL_FORMAT_: 'H',
            EMAIL_PERMISSION_STATUS_: 'I',
            EMAIL_ADDRESS_: emailID2,
            MARKET: currCode
        }

        var CMjar = new CookieJar( {expires: '', path: '/'} );
                                CMjar.put('storeE', JSON.stringify(objStore));
                                console.log('cookie stored');
    });

});
    </script>

