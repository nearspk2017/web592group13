  console.log($("#getlike").contents()); 
  },10000);

function showIndex(){
  $('.show_index').fadeOut(500);
  $('.hidden_index').fadeIn(500);
  $('.index_detail').fadeIn(500);
}

function hiddenIndex(){
  $('.show_index').fadeIn(500);
  $('.hidden_index').fadeOut(500);
  $('.index_detail').fadeOut(500);
}

var share_num = 0;
//var url = "https://graph.facebook.com/?id="+window.location.href.split("?")[0];

$.getJSON(url, function(data) {
  if (data['share']['share_count'] > 0) share_num += data['share']['share_count'];
  url = "https://graph.facebook.com/?id=http://sistacafe.com/summaries/541";
  $.getJSON(url, function(data) {
    if (data['share']['share_count'] > 0) share_num += data['share']['share_count'];
    if( share_num >= 1000 ) {
        share_count = $("#fb-shares")
        console.log(share_num);
        share_count.html(share_num)
        share_count.css("margin-right", "6px")

        share_count = $(".summary .fb-shares-count .count")
        share_count.html(share_num)
        share_count.css("margin-right", "6px")
        $(".summary .fb-shares-count").css("display", "block")

        share_count = $(".popup.share .content .fbshare-count .count")
        share_count.html(share_num)
        share_count.css("margin-right", "6px")
        $(".popup.share .content .fbshare-count").css("display", "block")
    }
    }
    );
});


var loading_img = "data:image/gif;base64,";


$(function(){

    var cnt_page = 2;
    var sum_url = location.href;

    $('.summary-contents').infinitescroll({
        debug        : false,
        navSelector  : ".pagination",
        nextSelector : ".pagination .page a",
        itemSelector : ".summary-source",
        loading: {
            finishedMsg: "",
            msgText: "Loading...",
            img: loading_img,

            finished: function(opts) {
              opts.loading.msg.fadeOut(opts.loading.speed);
              $("img").lazyload({
                container: $(".main-container")
              });
              $.ajax({ url: '/summaries/541/count', method: 'post',
                success: function(data) {}
              });
              //$(".summary-source").last().css("display","none");
              //$(".summary-contents").append('<div id="boxmore" style="text-align: center;height: 180px;padding-top: 80px;margin-top: -80px; background: linear-gradient(to bottom, rgba(255,255,255,0) 0%, white 40%, white 100%);"> <img style="width:30%; margin:auto;" id="readmore" src="/assets/button/read_more-f94efec5f6e89c92dc95f408b56aa8c04b4850165d91b9b4b4d35fc7b9b55b90.png" alt="Read more f94efec5f6e89c92dc95f408b56aa8c04b4850165d91b9b4b4d35fc7b9b55b90" /> </div>');
              ga('send', 'pageview', {'page': location.pathname+"?page="+cnt_page, 'title': window.document.title+'?page='+cnt_page});
              if ($(".summary")[0]) {
                category = $(".summary").attr('category');
                if (category && category != "") ga('send', 'event', 'category', 'showed', category, {'nonInteraction': 1});
              }
              cnt_page++;
              if(cnt_page > "2") $('.summary-contents').infinitescroll('unbind');
              renderTweetButtons();

              if(window.instgrm)  window.instgrm.Embeds.process();
              if(index_work != ''){
                if(typeof $(index_work).offset() === "undefined"){
                  $('.summary-contents').infinitescroll('retrieve');
                }else{
                  $('html,body').animate({scrollTop: $(index_work).offset().top-50},'slow');
                  index_work = '';
                }
              }

              setTimeout(function(){
                var classList = document.getElementsByClassName('map');
                for (var i = 0; i < classList.length; i++) {
                    console.log(classList[i]);
                    initMap_autoload(classList[i]);
                }
              },0)
            }
        },
        state: {
          currPage: cnt_page-1
        },
        errorCallback: function () {
          $('#infscr-loading').hide();
        },
        maxPage:"2",
        path: function(){
          return sum_url+"?page="+cnt_page;
        },
        bufferPx: 3000,
        },function(newElements) {

        }
      );

    $('.summary-contents').on('click', '#readmore', function(){
        //$('#boxmore').remove();
        //$(".summary-source").last().fadeIn(200);
        //if(cnt_page <= "2") {
          //$('.summary-contents').infinitescroll('bind');
        //}
    });
});

function initMap_autoload(map_class) {
  var myLatLng = {lat: parseFloat($("#"+map_class.id).attr("data-lat")), lng: parseFloat($("#"+map_class.id).attr("data-long"))};
  var map = new google.maps.Map(document.getElementById(map_class.id), {
    center: myLatLng,
    zoom: 14
  });

  var icon = {
          url: "/assets/cafe/icon_event_location-2bca99ebb3f749ce98b5df7abf0e0da1ed70c11d524c2c804d8209f47feef999.png",
          size: new google.maps.Size(71, 71),
          origin: new google.maps.Point(0, 0),
          anchor: new google.maps.Point(17, 34),
          scaledSize: new google.maps.Size(45, 45)
        };

  var marker = new google.maps.Marker({
      position: myLatLng,
      map: map,
      icon: icon
    });
}

var first_gal = true;

$("#b_con").on('click',function(){
  history.replaceState(null, '', '/summaries/541');
  $(this).css("display","none");
  $("#b_gal").css("display","block");
  $('.summary-contents').css("display","block");
  $('.album').css("display","none");
});

$("#b_gal").on('click',function(){
  if(first_gal) {
    $('.album').html("<div style='text-align: center; margin: 30px 0px;'><img src="+loading_img+"></div>");
    $.ajax({ url: '/gallery/album/541', method: 'post',
      success: function(data) {
        $('.album').html(data);
        $("img").lazyload({
          container: $(".main-container")
        });
        ga('send', 'pageview', '/gallery/album/541');
        first_gal = false;
        ga('send', 'event', 'uppv', 'switch', 'to_gallery');
      }
    });
  }
  history.pushState(null, '', '/gallery/album/541');
  $(this).css("display","none");
  $("#b_con").css("display","block");
  $('.summary-contents').css("display","none")
  $('.album').css("display","block")
});

$.ajax({ url: '/summaries/541/comments_tags',
success: function(data) {
  var theDiv = document.getElementById("cmc");
  var likeDiv = document.getElementById("like_add");
  if ($(data).filter('.comment_source').html() != undefined) theDiv.innerHTML += $(data).filter('.comment_source').html();
  if ($(data).filter('.like_source').html() != undefined) likeDiv.innerHTML += $(data).filter('.like_source').html();
  var tmp_comment_btn = $('#new_comment input[name=commit]').attr('value');
  $('body').on('submit', '#new_comment', function(){
     $('#new_comment input[name=commit]').attr('value','กำลังส่ง....');
     $('#new_comment input[name=commit]').attr('disabled', 'disabled');
     $('.form-error').remove();
  });

  $('body').on("contentchanged", ".comments", function(){
      $('#comment_contents, .input-name').val('');
      $('#new_comment input[name=commit]').attr('value',tmp_comment_btn);
      $('#new_comment input[name=commit]').removeAttr('disabled');
      var prev_no = parseInt($( ".comments .comment:eq(-2)" ).find(".detail b").text().substr(2));
      if(!prev_no){
        prev_no = 0;
      }
      prev_no++;
      $( ".comments .comment:eq(-1)" ).find(".detail b").text(" #" + prev_no);
  });

  $(".chosen-select").chosen({allow_single_deselect: true,no_results_text: 'No results matched',width: '200px', search_contains: true});
  if ($('.delete-tag')[0]) {
    $(".delete-tag").on("click",function(){
      if (confirm("\u0e15\u0e49\u0e2d\u0e07\u0e01\u0e32\u0e23\u0e25\u0e1a\u0e41\u0e17\u0e47\u0e01\u0e2b\u0e23\u0e37\u0e2d\u0e44\u0e21\u0e48\uff1f"+$(this).parent().attr('data-tag-id'))) {
        $(this).parent().remove();
        return true;
      }
      else return false;
    })
  }
}});</script><footer id="footer"><div class="categories-foot"><div class="foot-label"><p>Categories</p></div><table class="category-list"><tr><td><a href="/categories/%E0%B9%81%E0%B8%95%E0%B9%88%E0%B8%87%E0%B8%AB%E0%B8%99%E0%B9%89%E0%B8%B2,%E0%B9%80%E0%B8%84%E0%B8%A3%E0%B8%B7%E0%B9%88%E0%B8%AD%E0%B8%87%E0%B8%AA%E0%B8%B3%E0%B8%AD%E0%B8%B2%E0%B8%87"><div class="footer-category-item"><img src="https://cache.gmo2.sistacafe.com/images/uploads/category/icon/7/1450259503-sistacafe_%E0%B9%81%E0%B8%95%E0%B9%88%E0%B8%87%E0%B8%AB%E0%B8%99%E0%B9%89%E0%B8%B2_%E0%B9%80%E0%B8%A1%E0%B8%84%E0%B8%AD%E0%B8%B1%E0%B8%9E_makeup_cosmetic.png" alt="1450259503 sistacafe %e0%b9%81%e0%b8%95%e0%b9%88%e0%b8%87%e0%b8%ab%e0%b8%99%e0%b9%89%e0%b8%b2 %e0%b9%80%e0%b8%a1%e0%b8%84%e0%b8%ad%e0%b8%b1%e0%b8%9e makeup cosmetic" /><div class="name">แต่งหน้า</div></div></a></td><td><a href="/categories/%E0%B9%81%E0%B8%9F%E0%B8%8A%E0%B8%B1%E0%B9%88%E0%B8%99"><div class="footer-category-item"><img src="https://cache.gmo2.sistacafe.com/images/uploads/category/icon/5/1450259430-sistacafe_%E0%B9%81%E0%B8%9F%E0%B8%8A%E0%B8%B1%E0%B9%88%E0%B8%99_fashion.png" alt="1450259430 sistacafe %e0%b9%81%e0%b8%9f%e0%b8%8a%e0%b8%b1%e0%b9%88%e0%b8%99 fashion" /><div class="name">แฟชั่น</div></div></a></td><td><a href="/categories/%E0%B8%84%E0%B8%A7%E0%B8%B2%E0%B8%A1%E0%B8%87%E0%B8%B2%E0%B8%A1"><div class="footer-category-item"><img src="https://cache.gmo2.sistacafe.com/images/uploads/category/icon/10/1450259591-sistacafe_%E0%B8%84%E0%B8%A7%E0%B8%B2%E0%B8%A1%E0%B8%87%E0%B8%B2%E0%B8%A1_beauty.png" alt="1450259591 sistacafe %e0%b8%84%e0%b8%a7%e0%b8%b2%e0%b8%a1%e0%b8%87%e0%b8%b2%e0%b8%a1 beauty" /><div class="name">ความงาม</div></div></a></td><td><a href="/categories/%E0%B8%97%E0%B8%A3%E0%B8%87%E0%B8%9C%E0%B8%A1,%E0%B8%97%E0%B8%B3%E0%B8%9C%E0%B8%A1"><div class="footer-category-item"><img src="https://cache.gmo2.sistacafe.com/images/uploads/category/icon/8/1450259639-sistacafe_%E0%B8%97%E0%B8%A3%E0%B8%87%E0%B8%9C%E0%B8%A1_hairstyle.p