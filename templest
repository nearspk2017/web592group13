    if (a > 0){
      $(".sidebar-left").css("top", (a-10) * -1);
    }
    else{
      $(".sidebar-left").css("top" , 0 );
    }
  }
  else{
    if (a > 0){
      $(".sidebar-left").css("top", (a-130) * -1);
    }
    else{
      $(".sidebar-left").css("top" , 0 );
    }
  }
}

