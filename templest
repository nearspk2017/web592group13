    theDiv.innerHTML += data; 
    if(window.location.href.indexOf('/pass/') == -1){
      if (data.indexOf("fuzk") == -1) {
        $(document).on("contextmenu","img", function(e) {
            return false;
        });
      }
    }
  } });</script></div><div class="home" id="home"><p class="alert"></p><script src="https://js.mtburn.com/advs-instream.js"></script><script async="" src="//pagead2.googlesyndication.com/pagead/js/adsbygoogle.js"></script><script src="https://www.google.com/jsapi" type="text/javascript"></script><script async="" defer="" src="//platform.instagram.com/en_US/embeds.js"></script><script async="" defer="" src="https://maps.googleapis.com/maps/api/js?key=AIzaSyBrfookg-afhEQEuPygUS_4Ny6t_cLYCsc&amp;libraries=places"></script><div category="10_ความงาม" class="summary" id="summary" summary_id="541"><div class="summary-introduction" page="1"><div class="introduction"><div class="top-image"><img alt="รวม7สุดยอดครีมกันแดดถูกและดีขั้นเทพของสาวไทย" src="https://cache.gmo2.sistacafe.com/images/uploads/summary/image/541/1442910681-korean_20sunscreens_201.png" /><div class="summary-source"><a href="http://i1137.photobucket.com/albums/n514/chichibanban/2015/20150420%20sunblock%20overview/korean%20sunscreens%201.png" rel="nofollow" target="_blank">i1137.photobucket.com</a></div><div class="info"><div category-id="" class="summary-category"><div class="category-image"><img src="https://cache.gmo2.sistacafe.com/images/uploads/category/icon/10/1450259591-sistacafe_%E0%B8%84%E0%B8%A7%E0%B8%B2%E0%B8%A1%E0%B8%87%E0%B8%B2%E0%B8%A1_beauty.png" alt="1450259591 sistacafe %e0%b8%84%e0%b8%a7%e0%b8%b2%e0%b8%a1%e0%b8%87%e0%b8%b2%e0%b8%a1 beauty" /></div><div class="category-name">ความงาม</div></div><div class="title"><div class="table"><div class="cell"><h1 class="text clamp">รวม 7 สุดยอด ครีมกันแดด &quot;ถูกและดี&quot; ขั้นเทพ ของสาวไทย</h1><span class="edit-summary-btn" id="eintro" style="top:0"></span></div></div></div><div class="footer" id="foot_view"><div class="curator"><div class="name" style="font-weight:bold; font-size:14px;"><a href="/curators/4">Sista Peony</a></div><div class="spliter">|</div><time datetime="2015-05-27">May 27, 2015</time></div><div class="choco" style="font-weight:b