function goToByScroll(className) {
    var className = className.split(" ").join(".");
  if(typeof $("."+className).offset() === "undefined"){
    index_work = "."+className;
    $('.summary-contents').infinitescroll('retrieve');
    ga('send', 'event', 'uppv', 'indexing', 'current_page');
  }
  else {
    $('html,body').animate({scrollTop: $("."+className).offset().top-50},'slow');
    ga('send', 'event', 'uppv', 'indexing', 'next_page');
  }
}

